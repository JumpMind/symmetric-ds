<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" default-lazy-init="true">

    <bean id="registeredDataFilter" class="org.jumpmind.symmetric.ext.TestDataLoaderFilter">
        <property name="autoRegister" value="true" />
    </bean>

    <bean id="unRegisteredDataFilter" class="org.jumpmind.symmetric.ext.TestDataLoaderFilter">
        <property name="autoRegister" value="false" />
    </bean>

    <bean id="registeredNodeGroupTestDataFilter" class="org.jumpmind.symmetric.ext.NodeGroupTestDataLoaderFilter">
        <property name="nodeGroups">
            <list>
                <value>test-root-group</value>
            </list>
        </property>
    </bean>

    <bean id="unRegisteredNodeGroupTestDataFilter" class="org.jumpmind.symmetric.ext.NodeGroupTestDataLoaderFilter">
           <property name="nodeGroups">
            <list>
                <value>zulu</value>
            </list>
        </property>
    </bean>


</beans>