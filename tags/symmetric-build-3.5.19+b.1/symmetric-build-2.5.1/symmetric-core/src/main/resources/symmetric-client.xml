<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config />

    <import resource="classpath:/symmetric-properties.xml" />
    <import resource="classpath:/symmetric-database.xml" />
    <import resource="classpath:/symmetric-dialects.xml" />
    <import resource="classpath:/symmetric-services.xml" />
    <import resource="classpath:/symmetric-upgrade.xml" />
    <import resource="classpath:/symmetric-jobs.xml" />
    <import resource="classpath:/symmetric-jmx.xml" />
    <import resource="classpath:/symmetric-routers.xml" />
    <import resource="classpath:/symmetric-transform.xml" />
    <import resource="classpath:/symmetric-extensions.xml" />
    
</beans>