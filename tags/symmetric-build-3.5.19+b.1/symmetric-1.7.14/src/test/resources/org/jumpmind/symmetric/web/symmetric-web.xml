<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <import resource="classpath:/symmetric-properties.xml" />
    
    <bean id="dataLoaderService"
        class="org.jumpmind.symmetric.service.mock.MockDataLoaderService" />
    <bean id="acknowledgeService"
        class="org.jumpmind.symmetric.service.mock.MockAcknowledgeService" />
    <bean id="outgoingBatchService"
        class="org.jumpmind.symmetric.service.mock.MockOutgoingBatchService" />
    <bean id="incomingBatchService"
        class="org.jumpmind.symmetric.service.mock.MockIncomingBatchService" />
    <bean id="dataExtractorService"
        class="org.jumpmind.symmetric.service.mock.MockDataExtractorService" />
    <bean id="nodeService"
        class="org.jumpmind.symmetric.web.MockNodeService" />
    <bean id="registrationService"
        class="org.jumpmind.symmetric.web.MockRegistrationService" />
        
    <import resource="classpath:/symmetric-web.xml" />

</beans>