    <build>
        <extensions>
            <extension>
                <groupId>org.apache.maven.wagon</groupId>
                <artifactId>wagon-webdav-jackrabbit</artifactId>
                <version>2.2</version>
            </extension>
        </extensions>
        <plugins>
            <plugin>
                <artifactId>maven-eclipse-plugin</artifactId>
                <version>2.9</version>
                <configuration>
                    <downloadSources>true</downloadSources>
                    <downloadJavadocs>false</downloadJavadocs>
                    <wtpversion>1.5</wtpversion>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                    <optimize>true</optimize>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-jar-plugin</artifactId>
                <version>2.3.2</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>test-jar</goal>
                        </goals>
                        <phase>test-compile</phase>
                    </execution>
                </executions>
                <configuration>
                    <outputDirectory>${basedir}/target</outputDirectory>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.1.2</version>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.10</version>
                <configuration>
                    <forkMode>always</forkMode>
                    <argLine>-Xms512m -Xmx1024m -Duser.timezone=America/New_York</argLine>
                    <systemProperties>
                        <property>
                            <name>test.root</name>
                            <value>${test.root}</value>
                        </property>
                        <property>
                            <name>test.client</name>
                            <value>${test.client}</value>
                        </property>
                    </systemProperties>
                    <additionalClasspathElements>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/ojdbc6.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/ojdbc5.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/ojdbc14.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/db2jcc.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/ifxjdbc.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/ifxlang.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/interclient.jar 
                        </additionalClasspathElement>
                        <additionalClasspathElement> ${user.home}/.symmetricds/lib/sqljdbc4.jar 
                        </additionalClasspathElement>
                    </additionalClasspathElements>
                </configuration>
            </plugin>
        </plugins>
    </build>
