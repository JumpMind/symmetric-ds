<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xsi:schemaLocation="    
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd"
  default-lazy-init="true">

  <util:map id="statisticServiceSql">
    <entry key="insertStatisticSql">
      <value>
        insert into ${sync.table.prefix}_statistic (node_id, host_name, statistic_name,
        capture_start_time, capture_end_time, statistic_total, statistic_count) values(?,?,?,?,?,?,?)
      </value>
    </entry>
  </util:map>

</beans>