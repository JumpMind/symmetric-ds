<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true">

	<bean id="transformService" class="org.jumpmind.symmetric.transform.TransformService"
		parent="abstractService">
		<property name="sql" ref="transformServiceSql" />
	</bean>

	<bean id="transformDataLoader" class="org.jumpmind.symmetric.transform.TransformDataLoader">
		<property name="dbDialect" ref="dbDialect" />
		<property name="transformService" ref="transformService" />
		<property name="parameterService" ref="parameterService" />
	</bean>

	<bean id="additiveColumnTransform"
		class="org.jumpmind.symmetric.transform.AdditiveColumnTransform">
		<property name="dbDialect" ref="dbDialect" />
	</bean>
	
    <bean id="constantColumnTransform"
		class="org.jumpmind.symmetric.transform.ConstantColumnTransform"/>

</beans>