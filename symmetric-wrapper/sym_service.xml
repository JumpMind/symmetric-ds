<!--
   To build sym_service.exe: 
   on Linux: launch4j/launch4jc sym_service.xml
   on Windows: launch4j/launch4j.exe sym_service.xml
-->
<launch4jConfig>
  <headerType>gui</headerType>
  <outfile>sym_service.exe</outfile>
  <jar>..\lib\symmetric-wrapper.jar</jar>
  <dontWrapJar>true</dontWrapJar>
  <errTitle>SymmetricDS</errTitle>
  <stayAlive>true</stayAlive>
  <jre>
    <path>%JAVA_HOME%;%PATH%</path>
    <requiresJdk>false</requiresJdk>
    <requires64Bit>false</requires64Bit>
    <minVersion>17</minVersion>
  </jre>
  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0</txtFileVersion>
    <fileDescription>SymmetricDS Service</fileDescription>
    <copyright>2024</copyright>
    <productVersion>1.0.0.0</productVersion>
    <txtProductVersion>1.0</txtProductVersion>
    <productName>SymmetricDS</productName>
    <companyName>Jumpmind, Inc</companyName>
    <internalName>sym_service</internalName>
    <originalFilename>sym_service.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
